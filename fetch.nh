#ifndef __INCLUDED_INST_FETCH_NH__
#define __INCLUDED_INST_FETCH_NH__

declare fetch
{
    // for memory;
    output address[32];
    input inst[32];

    // for internal
    output inst_out[32];
    input next_pc[32];

    // for memory
    func_out request(address);
    func_in enable(inst);

    // for internal
    func_in run(next_pc);
    func_out done(inst_out);
}

