#include "dummy_arbiter.nh"

declare dummy_arbiter_tb simulation
{
    output error_num[ERROR_NUM_BIT_WIDTH];
    func_out error(error_num);
}

module dummy_arbiter_tb
{
    dummy_arbiter arbiter;

    reg counter[10] = 0;
    counter++;

    if(counter == 10'h08)
    {
        arbiter.request_inst(0x8000_0000);
    }

    if(counter == 10'h10)
    {
        arbiter.store_w(0x8000_0010, 0x12345678);
    }

    if(counter == 10'h40)
    {
        _finish();
    }
}
